func main()
{
	'the language supports arbitrary mathematical expressions
	println (2 * 2) + 3 - 5 / 4 * (2 + 1 - (2 + 1) / 2)

	'functions and variables can be used in these expressions
	var bar = 3
	println (2 * foo()) - bar

	'unary expressions are supported
	var bok = 5
	bok = -bok
	println bok
}

func foo()
{
	return 6
}