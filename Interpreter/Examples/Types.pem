func main()
{
	'there are 6 main types in this language

	'number
	var a = 23
	println a

	'string
	var b = "yea"
	println b

	'boolean
	var c = true
	println c

	'array
	var d = [1, 2, "3"]
	println d[1]

	'null
	var e = null
	println e

	'function
	var f = func() { println "Hello, world" }
	f()

	'type checking can be done with the type() method
	println type(b)
	println type(c)

	'you can convert strings to numbers using the tonumber() method
	println tonumber("2") + 3

	'in many cases implicit casting is supported
	'string concatenation is a good example
	println "one " + 2 + " three " + true
}