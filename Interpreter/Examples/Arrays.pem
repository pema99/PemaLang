func main()
{
	'an array of numbers, arrays are zero-indexed
	var arr1 = [1, 5, 3]
	println arr1[0]
	println arr1[1]

	'arrays can contain different types
	var arr2 = ["foo", 2, true, "bar"]
	println arr2[0]
	println arr2[2]
	
	'arrays can grow to any size and are by nature dynamic
	var arr3 = [0, 1, 2, 3]
	arr3[3] = 4
	println arr3[3]
	arr3[99] = 99
	println arr3[99]
}